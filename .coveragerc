[run]
source = .
omit =
    */migrations/*
    */venv/*
    */env/*
    */tests/*
    */test_*.py
    manage.py
    */settings*.py
    */__init__.py
    */celery.py
    */wsgi.py
    */asgi.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = logs/coverage

[xml]
output = logs/coverage/coverage.xml
